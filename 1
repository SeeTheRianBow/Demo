         $(function(){
         	var mydate = new Date();
         	var t=mydate.toLocaleString();
         	var d;
         	d=Math.floor(Math.random()*(200-1)+1);
         	$("#time").text(t);
         	$("#Number").text(d+"号").css("font-size","30px");
         })
